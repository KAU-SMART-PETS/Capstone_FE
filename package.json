{
  "name": "reactCapsone",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "ios": "cd ios && react-native run-ios --mode 'Debug' --scheme reactCapsone --xcconfig ExtraArgs.xcconfig",
    "lint": "eslint .",
    "start": "react-native start",
    "prepare": "husky install",
    "clean": "rm -rf node_modules && yarn install",
    "android:clean": "cd android && ./gradlew clean",
    "ios:pods": "cd ios && pod install",
    "ios:fastlane": "cd ios && bundle exec fastlane ios beta",
    "script": "cd $(git rev-parse --show-toplevel) && cd scripts && ./run_scripts.sh"
  },
